{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["src/*"],
      "@admin/*": ["src/admin/*"],
      "@api/*": ["src/api/*"],
      "@client/*": ["src/client/*"],
      "@hooks/*": ["src/hooks/*"],
      "@interface/*": ["src/interface/*"],
      "@shared/*": ["src/shared/*"],
      "@store/*": ["src/store/*"]
    },
    "noEmit": true
  },
  "include": ["src/**/*"],
  "exclude": ["mode_modules", "dist"]
}
